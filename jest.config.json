{
  "testEnvironment": "jsdom",
  "collectCoverage": true,
  "moduleFileExtensions": [
    "js",
    "mjs",
    "ts"
  ],
  "transform": {
    "^.+\\.js$": "babel-jest",
    "^.+\\.mjs$": "babel-jest",   
    "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
  },
  "preset": "jest-puppeteer",
  "coverageReporters": ["clover", "json", "lcov", "text", "json-summary"]
}